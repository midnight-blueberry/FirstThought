{
  "name": "firstthought",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env BABEL_CONFIG_FILE=config/babel.config.js npx expo start -c --config config/app.json",
    "build:google-play": "BABEL_CONFIG_FILE=config/babel.config.js npx expo prebuild --config config/app.json && BABEL_CONFIG_FILE=config/babel.config.js npx eas build -p android --config config/eas.json",
    "build:testing": "BABEL_CONFIG_FILE=config/babel.config.js eas build -p android --profile testing --config config/eas.json",
    "build:apk": "BABEL_CONFIG_FILE=config/babel.config.js expo run:android --config config/app.json",
    "ios": "BABEL_CONFIG_FILE=config/babel.config.js expo run:ios --config config/app.json",
    "web": "BABEL_CONFIG_FILE=config/babel.config.js expo start --web --config config/app.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --config config/eslint.config.mjs",
    "test": "jest --config config/jest.config.js",
    "typecheck": "tsc -p config/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-vector-icons/ionicons": "^12.3.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^2.5.2",
    "@react-navigation/native": "^7.1.6",
    "crypto-js": "^4.2.0",
    "expo": "^53.0.19",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@types/jest": "^29.5.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
